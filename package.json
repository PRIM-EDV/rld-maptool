{
  "name": "rld-maptool",
  "version": "0.5.1",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron-build": "ng build --base-href && electron dist",
    "electron": "electron ./src/electron/electron.dev",
    "build": "ng build && node scripts/bundle.js",
    "build-windows": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=win32 --arch=all --overwrite",
    "build-linux": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=linux --arch=all --overwrite",
    "deploy-windows": "npm run build-windows && node scripts/deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ionic-native/ble": "^5.26.0",
    "@ionic-native/core": "^5.26.0",
    "core-js": "^3.6.5",
    "electron": "^9.0.0",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "uuid": "^8.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "~14.0.6",
    "@types/uuid": "^8.0.0",
    "archiver": "^4.0.1",
    "concurrently": "^5.2.0",
    "cross-var": "^1.1.0",
    "electron-packager": "^14.2.1",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "^3.8.3"
  }
}
