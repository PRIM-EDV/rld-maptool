{
  "name": "rld-maptool",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "electron-build": "ng build --base-href && electron dist",
    "electron": "electron ./src/electron/electron.dev",
    "build": "ng build && node scripts/bundle.js",
    "build-windows": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=win32 --arch=all --overwrite",
    "build-linux": "npm run build && cross-var electron-packager dist/rld-maptool maptool-$npm_package_version --out=build --platform=linux --arch=all --overwrite",
    "deploy-windows": "npm run build-windows && node scripts/deploy.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ionic-native/ble": "^5.6.0",
    "@ionic-native/core": "^5.6.0",
    "core-js": "^2.5.4",
    "electron": "^5.0.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "~8.9.4",
    "@types/uuid": "^3.4.4",
    "archiver": "^3.0.3",
    "concurrently": "^4.1.0",
    "cross-var": "^1.1.0",
    "electron-packager": "^13.1.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
}
